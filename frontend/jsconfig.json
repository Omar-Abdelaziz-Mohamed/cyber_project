{
    "compilerOptions": {
        "checkJs": true,
        "jsx": "react-jsx",
        "baseUrl": "src",
        "resolveJsonModule": true,
        "lib": [
            "es6",
            "dom",
            "es2017"
        ],
        "module": "esnext",
        "target": "esnext",
        "moduleResolution": "node",
        "types": ["vite/client"]
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "build"
    ]
}
